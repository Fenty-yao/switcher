{
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "module": "commonjs",
        "outDir": "public",
        "rootDir": "src",
        "strict": true,
        "allowJs": true,
        "esModuleInterop": true
    }
}